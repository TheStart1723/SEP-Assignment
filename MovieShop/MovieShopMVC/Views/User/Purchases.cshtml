@model IEnumerable<ApplicationCore.Models.PurchaseResponseModel>

@{
    ViewData["Title"] = "Purchased Movies";
}

<div class="container-fluid">
    <div class="row">
        @foreach (var purchasedMovie in Model)
        {
            <div class="col-xl-2 col-sm-4 col-lg-3 col-6">
                <partial model="purchasedMovie" name="_MovieCardPurchased" />
            </div>
        }
    </div>
</div>